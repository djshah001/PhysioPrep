{
  "expo": {
    "name": "PhysioPrep",
    "slug": "physioprep",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "scheme": "physioprep",
    "icon": "./assets/icon-main.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon2.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darshanx01.physioprep"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-main.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.darshanx01.physioprep"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4cee1934-6ca7-4251-9842-18db1c9174e0"
      }
    }
  }
}
